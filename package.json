{
  "name": "provincle",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest --config ./vitest.config.ts",
    "cover": "vitest --config ./vitest.config.ts run --coverage --coverage.extension=ts --coverage.extension=tsx",
    "format": "prettier --write src/**/*.{ts,tsx} && prettier --write src/components/**/*.test.tsx && prettier --write src/test/**/*.{ts,tsx} && prettier --write ./*.{js,cjs,ts}"
  },
  "dependencies": {
    "he": "^1.2.0",
    "react": "^18.3.1",
    "react-autosuggest": "^10.1.0",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.3.1",
    "@testing-library/react": "^16.0.0",
    "@types/he": "^1.2.3",
    "@types/react": "^18.3.3",
    "@types/react-autosuggest": "^10.1.11",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^2.0.2",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "gh-pages": "^6.1.1",
    "jsdom": "^24.1.0",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.4",
    "testing-library": "^0.0.2",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vitest": "^2.0.2"
  }
}
